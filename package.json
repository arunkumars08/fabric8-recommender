  "description": "The Fabric8 Recommendation components",
  "module": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "angular2",
    "webpack",
    "typescript"
  ],
  "contributors": [
    {
      "name": "Arunkumar S",
      "email": "asrisail@redhat.com"
